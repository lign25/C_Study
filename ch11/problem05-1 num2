#include<stdio.h>

int main(){
    double num1,num2;
    scanf("%lf %lf",&num1,&num2);
    printf("두 수의 덧셈:%f\n",num1+num2);
    printf("두 수의 뺄셈:%f\n",num1-num2);
    printf("두 수의 곱셈:%f\n",num1*num2);
    printf("두 수의 나눗셈:%f",num1/num2);
    return 0;
}
    // 두 개의 실수를 입력 받아서 double형 변수에 저장하고 두 수의 사칙연산 결과를 출력해보는 프로그램
